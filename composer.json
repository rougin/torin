{
  "name": "rougin/torin",
  "description": "Simple inventory management.",
  "homepage": "https://github.com/rougin/torin",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "name": "Rougin Gutib"
    }
  ],
  "require":
  {
    "php": ">=5.4.0",
    "illuminate/database": "*",
    "rougin/dexterine": "dev-master",
    "rougin/dexterity": "dev-master",
    "rougin/fortem": "dev-master",
    "rougin/gable": "dev-master",
    "rougin/onion": "dev-master",
    "rougin/validia": "dev-master",
    "rougin/weasley": "~0.7",
    "tedivm/jshrink": "~1.0",
    "vlucas/phpdotenv": "~2.0"
  },
  "require-dev":
  {
    "phpunit/phpunit": "~4.2|~5.7|~6.0|~7.0|~8.0|~9.0",
    "robmorgan/phinx": "~0.1",
    "sanmai/phpunit-legacy-adapter": "~6.1|~8.0"
  },
  "autoload":
  {
    "psr-4":
    {
      "Rougin\\Torin\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Rougin\\Torin\\": "tests"
    }
  },
  "scripts":
  {
    "analyze": "phpstan analyse",
    "restyle": "php-cs-fixer fix --config=phpstyle.php",
    "test": "phpunit"
  }
}
